<%layout('../layout')%>
<!-- Page Content -->
<script src="/contract/vote.js"></script>
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <h1 class="page-header">address</h1>                
            <select id="accountSelect"></select>
            <input type="button" class="btn btn-primary" id="btn-account" value="설정"><br><br>
            <div class="col-lg-12">
                <div class="col-lg-4">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            addVote Test
                        </div>
                        <div class="panel-body">
                            <input type="text" id="vName" />
                            <input type="button" class="btn btn-primary" id="btn-addVote" value="추가">
                            <br>
                            <select id="voteList"></select>
                            <input type="button" class="btn btn-primary" id="btn-getVote" value="조회">
                            
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="col-lg-4">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            candidate Test
                        </div>
                        <div class="panel-body">
                            <h1 id="vote-seq"></h1>
                            <p class="text-primary" style="display: inline">후보명</p>
                            <input type="text" id="cName" />
                            <br>
                            <p class="text-primary" style="display: inline">이미지경로</p>
                            <input type="text" id="imgPath" />
                            <br>
                            <input type="button" class="btn btn-primary" id="btn-addCandidate" value="추가">
                            
                            <input type="button" class="btn btn-primary" id="btn-getCandidate" value="조회">
                            
                            <table class="table table-striped table-bordered table-hover dataTable no-footer" id="dataTables-example" role="grid" aria-describedby="dataTables-example_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 70px;">
                                                name
                                            </th>
                                            <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 100px;">
                                                imgPath
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="candidate-tbody">
                                    </tbody>
                                </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="col-lg-4">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            vote Test
                        </div>
                        <div class="panel-body">
                            <h1 id="vote-seq"></h1>
                            <select multiple class="form-control" id="candidates">
                                </select>
                            <p class="text-primary" style="display: inline">투표자</p>
                            <input type="text" id="vName" />
                            <br>
                            <input type="button" class="btn btn-primary" id="btn-voting" value="투표">
                            
                            <input type="button" class="btn btn-primary" id="btn-getVote" value="조회">
                            
                            <table class="table table-striped table-bordered table-hover dataTable no-footer" id="dataTables-example" role="grid" aria-describedby="dataTables-example_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 70px;">
                                                name
                                            </th>
                                            <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 100px;">
                                                imgPath
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="candidate-tbody">
                                    </tbody>
                                </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>

<!-- /#page-wrapper -->
